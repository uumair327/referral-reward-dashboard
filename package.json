{
  "name": "referral-reward-dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --base-href /referral-reward-dashboard/",
    "build:github-pages": "ng build --configuration production --base-href /referral-reward-dashboard/ && cp dist/referral-reward-dashboard/browser/index.html dist/referral-reward-dashboard/browser/404.html && echo '' > dist/referral-reward-dashboard/browser/.nojekyll",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:coverage": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "e2e:headless": "cypress run",
    "serve:ssr:referral-reward-dashboard": "node dist/referral-reward-dashboard/server/server.mjs",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/referral-reward-dashboard/browser/stats.json",
    "deploy:github-pages": "npm run build:github-pages",
    "precommit": "npm run lint && npm run test:ci"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.8",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.8",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/service-worker": "^20.3.0",
    "@angular/ssr": "^20.3.5",
    "express": "^5.1.0",
    "ngx-quill": "^28.0.1",
    "quill": "^2.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/animations": "^20.3.4",
    "@angular/build": "^20.3.5",
    "@angular/cli": "^20.3.5",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.4",
    "@cypress/schematic": "^4.1.4",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "axe-core": "^4.11.0",
    "cypress": "^15.4.0",
    "cypress-axe": "^1.7.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}